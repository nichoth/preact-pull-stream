{
  "name": "preact-pull-stream",
  "description": "Create a duplex stream from a preact component",
  "version": "0.0.2",
  "main": "dist/index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags && npm publish",
    "build": "browserify -t babelify > dist/index.js",
    "dev": "budo example/index.js --live -- -t babelify",
    "test": "browserify example/index.js -t babelify | tape-run"
  },
  "dependencies": {
    "pull-abortable": "^4.1.1",
    "pull-notify": "^0.1.1",
    "pull-stream": "^3.6.1",
    "pull-stream-util": "0.0.0",
    "xtend": "^4.0.1"
  },
  "peerDependencies": {
    "preact": "^8.2.5"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "budo": "^10.0.4",
    "eslint": "^4.8.0",
    "eslint-config-minlint": "^2.0.0",
    "preact": "^8.2.5",
    "pull-cat": "^1.1.11",
    "pull-scan": "^1.0.0",
    "tape": "^4.8.0",
    "tape-run": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/preact-pull-stream.git"
  },
  "keywords": [
    "preact",
    "pull",
    "stream",
    "pull-stream"
  ],
  "author": "nichoth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/preact-pull-stream/issues"
  },
  "homepage": "https://github.com/nichoth/preact-pull-stream#readme"
}
